{
  "name": "<%= projectName %>",
  "version": "0.1.0",
  "description": "",
  "main": "lib/index.js",
  "jsnext:main": "src/index.ts",
  "types": "lib",
  "type-check": "tsc",
  "scripts": {
    "test": "cross-env BROWSER=Chrome karma start karma.conf.js",
    "test-jsdom": "cross-env BROWSER=jsdom karma start karma.conf.js --singleRun=true",
    "coverage": "nyc report --reporter=lcov",
    "es5": "rm -rf ./lib && tsc -p tsconfig.json",
    "prepublishOnly": "npm run test && npm run es5",
    "ci": "npm run test && npm run coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yusangeng/<%= projectName %>.git"
  },
  "keywords": [],
  "author": {
    "name": "yusangeng",
    "email": "yusangeng@outlook.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/dateformat": "^3.0.0",
    "@types/lodash": "^4.14.120",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.19",
    "chai": "^4.2.0",
    "cross-env": "^5.2.0",
    "jsdom": "^13.2.0",
    "karma": "^4.0.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jsdom-launcher": "^7.0.0",
    "karma-mocha": "^1.3.0",
    "karma-webpack": "^3.0.5",
    "coveralls": "^3.0.2",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "source-map-support": "^0.5.10",
    "ts-node": "^8.0.2",
    "typescript": "^3.2.4"
  },
  "dependencies": {
    "tslib": "^1.9.3"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "include": [
      "src"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  }
}
