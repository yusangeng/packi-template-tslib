{
  "name": "w",
  "version": "0.1.0",
  "description": "A typescript library.",
  "main": "lib/index.js",
  "jsnext:main": "src/index.ts",
  "types": "lib/",
  "type-check": "tsc",
  "scripts": {
    "lint": "eslint --fix ./src && prettier --write ./src",
    "test": "nyc mocha --require ts-node/register test/*.spec.ts",
    "coverage": "nyc report --reporter=lcov",
    "es5": "rm -rf ./lib && tsc -p tsconfig.json",
    "prepublishOnly": "npm run lint && npm run test && npm run es5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yusangeng/<%= projectName %>.git"
  },
  "keywords": [],
  "author": {
    "name": "yusangeng",
    "email": "yusangeng@outlook.com"
  },
  "license": "MIT",
  "devDependencies": {
    "packi-lint-config": "^0.1.11",
    "@types/chai": "^4.2.14",
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.12",
    "chai": "^4.2.0",
    "coveralls": "^3.1.0",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "source-map-support": "^0.5.19",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "tslib": "^2.0.3"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "include": [
      "src"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  },
  "publishConfig": {
    "registry": "https://registory.npmjs.org/"
  }
}
